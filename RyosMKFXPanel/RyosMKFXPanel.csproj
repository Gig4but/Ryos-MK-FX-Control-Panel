  <Target Name="PostBuild" AfterTargets="PostBuildEvent">
    <Exec Command="xcopy &quot;$(ProjectDir)Devices\Roccat\RyosTalkFX\CroccatTalkWrapper\win32-x86-64\talkfx-c.dll&quot; &quot;$(OutDir)&quot; /y /d&#xD;&#xA;xcopy &quot;$(SolutionDir)packages\NAudio.1.10.0\lib\net35\NAudio.dll&quot; &quot;$(OutDir)&quot; /y /d" />
  </Target>

